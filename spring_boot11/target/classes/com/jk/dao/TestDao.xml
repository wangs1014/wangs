<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.jk.dao.TestDao" >
<select id="sele" resultType="com.jk.model.User">
select * from t_user
</select>
    <delete id="delete">
    delete from t_user where id=#{id}
    </delete>
    <insert id="add">
    insert into t_user values(#{user.id},#{user.name},#{user.region})
    </insert>
    <update id="update">
    update t_user set name=#{name},region=#{monreg} where id=#{id}
    </update>
    <select id="toindexupdate" resultType="com.jk.model.User">
select * from t_user where id=#{id}
</select>
    
</mapper>  